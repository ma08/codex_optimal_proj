["\n\nimport sys\nimport math\ninput = sys.stdin.readline\n\nn = int(input())\nt, a = map(int, input().split())\nh = [int(input()) for i in range(n)]\n\ndiff = []\nfor i in range(n):\n    diff.append(abs(t - h[i] * 0.006 - a))  # abs:absolute value\n\nprint(diff.index(min(diff)) + 1)\n", "\n# === import ======================================================================================================================================================\nimport sys\n\n# === const =======================================================================================================================================================\n\n# === functions ===================================================================================================================================================\n\n# === classes =====================================================================================================================================================\n\n# === main ========================================================================================================================================================\ndef main():\n    n = int(input())\n    t, a = map(int, input().split())\n    h = list(map(int, input().split()))\n\n    dif = [abs(t - h[i] * 0.006 - a) for i in range(n)]\n    ans = dif.index(min(dif)) + 1\n    print(ans)\n\n# =====================================================================================================================================================================\nif __name__ == \"__main__\":\n    main()\n", "\n# === import ======================================================================================================================================================\nimport sys\n\n# === const =======================================================================================================================================================\n\n# === functions ==========================================================